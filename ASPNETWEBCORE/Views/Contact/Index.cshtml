@{
    ViewData["Title"] = "Contact";
}
<!DOCTYPE html>
<html lang="sv-se">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- CSS only -->
    <script src="https://kit.fontawesome.com/41c8b474e2.js" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/contact.css">
    <title>Contact Us</title>
</head>
<body>
    <div class="bg-blue-gradient">
        <div class="container">
            <!-- header -->
            <div class="header-grid">
                <div id="branding">
                    <h1>OmniVus.</h1>
                    <p>IT Service & Technology</p>
                </div>
                <nav id="top-menu">
                 <a href="mailto:info@domain.com">info@domain.com</a>

                    @if (SignInManager.IsSignedIn(User))
                    {
                        <div id="mydisplayname">
                            <a href="#">@User.FindFirst("DisplayName").Value</a>
                        </div>
                        <div id="myprofilename">
                        <a asp-controller="Profile" asp-action="Index">My Profile</a>
                        </div>

                        @if (User.IsInRole("admin"))
                        {
                            <a asp-controller="Role" asp-action="Index">Handle Roles</a>
                        }                      
                        
                    }
                    else
                    {
                    <div class="social">
                        <a href="https://facebook.com">facebook</a>
                        <a href="https://twitter.com">twitter</a>
                        <a href="https://linkedin.com">linkedin</a>
                        <a href="https://youtube.com">youtube</a>
                        </div>
                    }   
                </nav>
                <nav id="main-menu">
                    <div class="menu-items">
                        <a asp-controller="Home" asp-action="Index">Home</a>
                        <a asp-controller="Services" asp-action="Index">Services</a>
                        <a asp-controller="Contact" asp-action="Index">Contact</a>
                    </div>
                    
                    @if (SignInManager.IsSignedIn(User))
                    {
                        <a asp-controller="Authentication" asp-action="SignOut" class="btn btn-main">Logout</a>
                    }
                    else
                    {
                        <a asp-controller="Authentication" asp-action="SignIn" class="btn btn-main">Login</a>
                    }

                    
                </nav>
            </div>
            <div id="showcase">
                <div class="service">
                    <img src="/images/Contact Us.svg" id="pic1"> <br>               
                    <img src="/images/Home    Services.svg" id="pic2">
                </div>
            </div>
        </div> 
        </div>
        <!-- Showcase-->
  <div class="bg-gray-gradient"> 
                <section id="form">       
                            <div class="container">                              
                                    <div id="sides">
                                        <div id="leftside">
                                            <div class="bottombg">        
                                                <img src="/images/Get In Touch.png" id="img1">
                                                <img src="/images/line.svg" id="img2">  
                                                    <div id="texting">                             
                                                        <p>Estimate for your <br> projects.</p>
                                         
                                                        <div id="placeholder">
                                                            <div class="rows">
                                                                <div class="mb-3" style="width: 600px">
                                                                    <input type="text" autocomplete="off" onkeyup="validateName()" class="form-control success" id="firstnametest" placeholder="Enter your name" aria-label="First name"> 
                                                                    <span id="name-error"></span>
                                                                </div>
                                                                    <div class="mb-3">
                                                                            <input type="text" autocomplete="off" onkeyup="validateEmail()" class="form-control error" id="lastnametest" placeholder="Enter your email" aria-label="Last name">
                                                                            <span id="email-error"></span> 
                                                                    </div>
                                                            </div>
                                                                    <form class="mb-3">
                                                                        <input type="text" autocomplete="off" onkeyup="validateMessage()" class="form-control" id="floatingInputValue" placeholder="Enter your message" aria-label="Comments" >                                              
                                                                        <span id="message-error"></span>                                                                    
                                                                    </form>  
                                                                    <button type="submit" onclick="validateForm()" id="buttonmoves" class="btn btn-primary">Submit Now</button> 
                                                                    <span id="submit-error"></span>   
                                                                    <span id="submit-success"></span>  
                                                                    </div>                              
                                                                        </div>  
                                                                    </div>
                                                                </div>       
                                                                    <div id="rightside">                                                                                
                                                            <div class="groupblue">
                                                                <img src="/images/groupblue.svg">
                                    </div>
                            </div>   
                    </section>  
                </div>
                    

                            <div id="finish">
                                    <div class="bg-blackser">
                                        <div class="container">
                                            <div class="cataloge">
                                            <div class="Omnivus">
                                                <h1>OmniVus.</h1>
                                                <h2>The web has changed a lot since we posted our<br> first article back in 2007. The team at Mattin-<br>Lassi Group has changed too, as have the<br> things that we bring to our community<br>
                                                    conferences, books, and our membership<br> added to the online magazine. 
                                                </h2>
                                                    <h3>One thing that hasn’t changed is that we’re a<br> small team — with most of us not working</h3>
                                            </div>
                                            <div class="Workinghours">
                                                <h1>Working Hours</h1>
                                                <h2>Monday - Friday: 7:00 - 17:00<br>
                                                    Saturday: 7:00 - 12:00<br>
                                                    Sunday and holidays: 8:00 - 10:00</h2>

                                                   <h3> 
                                                       <font color="#FFFFFF">For more then 15 years,</font>
                                                    IT Service has been a <br> reliable partner in the field of logistics and <br> cargo forwarding.</h3>
                                                </div>
                                                <img src="/images/bubbles.svg" id="bubbles">
                                            </div>
                                            <div class="copyright">
                                                <p>Copyright © Mattin-Lassei Group AB - 2022</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>  


                        

            
            
            

        
    <!-- <div style="height: -500px"></div> -->

    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="js/contact.js"></script>
</body>
</html>